@model List<MemberVm>

@{
    ViewData["Title"] = "Member List";
}

<h1>Member List</h1>

<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Member Type</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var member in Model)
    {
        <tr>
            <td>@member.Name</td>
            <td>@member.Email</td>
            <td>@member.Membertypes</td>
        </tr>
    }
    </tbody>
</table>
